 <% if(excuse != null ) { %>
    <div class="container">
        <div class="row">
            <div class="col-1 ">
              <h4>Http_code  </h4>
                <ul class="list-group list-group-flush">
                  <% excuse.forEach(function(excuses, index){ %>
                    <li class="list-group-item"><%= excuses.http_code %></li>
                  <% }) %>
                </ul> 
            </div>
            <div class="col-3 ">
                <h4> Tag </h4>
                  <ul class="list-group list-group-flush">
                    <% excuse.forEach(function(excuses, index){ %>
                      <li class="list-group-item"><%= excuses.tag %></li>
                    <% }) %>
                  </ul> 
              </div>
            
            <div class="col-8 ">
              <h4> Message </h4>
                <ul class="list-group list-group-flush">
                  <% excuse.forEach(function(excuses, index){ %>
                    <li class="list-group-item"><%= excuses.message %></li>
                  <% }) %>
                </ul> 
            </div>
        </div>
    </div>
    
    <% } else { %>
      <p>No item found.</p>
    <% } %>